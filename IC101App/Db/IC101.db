record(stringout, "$(P)$(R)Test")
{       
        field(DESC, "TEST - should return 1")
        field(DTYP, "stream") 
        field(OUT, "@IC101.proto test $(PORT)")
        field(PINI, "YES")
        field(SCAN, "10 second") 
}
