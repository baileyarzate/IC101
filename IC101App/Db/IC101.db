record(stringout, "$(P)$(R)ID")
{
    field(DESC, "IC101 Serial Information")
    field(DTYP, "stream")
    field(OUT, "@IC101.proto getID($(P)$(R)Manufactuer,$(P)$(R)Model,$(P)$(R)Serial_N,$(P)$(R)Firmware) $(PORT)")
    field(PINI, "YES")
    field(SCAN, "Passive")
}

